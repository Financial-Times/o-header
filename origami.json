{
    "description": "Responsive Financial Times page header",
    "origamiType": "module",
    "origamiVersion": 1,
    "support": "https://github.com/Financial-Times/o-header/issues",
    "supportStatus": "active",
    "browserFeatures": {
        "required": [
            "classList",
            "customevent",
            "fetch"
        ]
    },
    "ci": {
        "circle": "https://api.travis-ci.org/repos/Financial-Times/o-header/builds.json"
    },
    "demosDefaults": {
        "dependencies": ["o-fonts@^2.0.0"],
        "js": "demos/src/main.js",
        "sass": "demos/src/main.scss",
        "data": "demos/src/data.js"
    },
    "demos": [
        {
            "name": "header",
            "template": "demos/src/header.mustache",
            "expanded": true,
            "description": "",
            "documentClasses": "core"
        },
        {
            "name": "header-minimal",
            "template": "demos/src/header-minimal.mustache",
            "expanded": true,
            "description": "",
            "documentClasses": "enhanced"
        },
        {
            "name": "header-light",
            "template": "demos/src/header-light.mustache",
            "expanded": true,
            "description": "",
            "documentClasses": "enhanced"
        }
    ]
}
